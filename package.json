{
  "name": "neo4jquery",
  "version": "1.0.2",
  "description": "Build complex Neo4J cypher queries easily with a query builder and simple method chaining.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "declaration": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "initTest": "ava --init",
    "test": "ava-ts --watch",
